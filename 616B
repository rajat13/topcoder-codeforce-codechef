#include<iostream>
using namespace std;
int main(){
int n,m,i,j,loc,temp,temp2,temp3;
cin>>n>>m;
int a[n][m],b[n];
for(i=0;i<n;i++){
    for(j=0;j<m;j++){
        cin>>a[i][j];}
}
for(i=0;i<n;i++){
temp=a[i][0];
    for(j=0;j<m;j++){
        if(a[i][j]<temp){
        temp=a[i][j];
                         }
                        }b[i]=temp;
                }              
     temp3=b[0];
 for(i=0;i<n;i++){
      if(b[i]>temp3){
         temp3=b[i];
         loc=i;}
                 }
  temp2=a[loc][0];
   for(i=0;i<m;i++){
       if(a[loc][i]<temp2){
            temp2=a[loc][i];
               }}
cout<<temp2<<endl;
return 0;
